{
    "name": "smart-dato/alpigroup-sdh",
    "description": "Alpigroup SDH",
    "keywords": [
        "smart-dato",
        "laravel",
        "alpigroup-sdk"
    ],
    "homepage": "https://github.com/smart-dato/alpigroup-sdh",
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "ext-zip": "*",
        "asmit/resized-column": "^1.0",
        "barryvdh/laravel-dompdf": "^3.1",
        "bezhansalleh/filament-shield": "^3.1",
        "cerbero/json-parser": "^1.1",
        "croustibat/filament-jobs-monitor": "^2.5",
        "deeplcom/deepl-php": "^1.6",
        "filament/filament": "^3.2",
        "filament/forms": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "filament/spatie-laravel-settings-plugin": "^3.2",
        "filament/tables": "^3.2",
        "guzzlehttp/guzzle": "^7.2",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.15",
        "laravel/sanctum": "^3.2",
        "laravel/scout": "^10.0",
        "laravel/tinker": "^2.8",
        "league/flysystem-ftp": "^3.0",
        "maatwebsite/excel": "^3.1",
        "mailersend/laravel-driver": "^2.3",
        "malzariey/filament-daterangepicker-filter": "^2.6",
        "openai-php/laravel": "^0.10.1",
        "petercoles/multilingual-country-list": "^1.2",
        "pusher/pusher-php-server": "^7.2",
        "saloonphp/laravel-plugin": "^3.0",
        "sentry/sentry-laravel": "^3.3",
        "shuvroroy/filament-spatie-laravel-backup": "^2.1",
        "smart-dato/ombis-sdk": "dev-main",
        "smart-dato/sdh-shopware-sdk": "dev-main",
        "smart-dato/shopware6-sdk": "dev-master",
        "spatie/laravel-medialibrary": "^11.4",
        "spatie/laravel-permission": "^6.0",
        "spatie/ray": "^1.37",
        "webklex/laravel-pdfmerger": "^1.3",
        "wnx/laravel-backup-restore": "^1.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.13",
        "doctrine/dbal": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "filament/upgrade": "^3.2",
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "ombis-sdk" : {
            "type": "vcs",
            "url": "git@bitbucket.org:smartdato/ombis-sdk.git"
        },
        "shopware6-sdk" : {
            "type": "vcs",
            "url": "git@bitbucket.org:smartdato/shopware6-sdk.git"
        },
        "sdh-shopware-sdk" : {
            "type": "vcs",
            "url": "git@bitbucket.org:smartdato/sdh-shopware-sdk.git"
        }
    }
}
